# Increase volume
"pactl set-sink-volume @DEFAULT_SINK@ +2500"
   XF86AudioRaiseVolume

# Decrease volume
"pactl set-sink-volume @DEFAULT_SINK@ -2500"
   XF86AudioLowerVolume

# Mute volume
"pactl set-sink-mute @DEFAULT_SINK@ toggle"
   XF86AudioMute

# Play - Pause
"playerctl play-pause"
    XF86AudioPlay

# Next
"playerctl next"
    XF86AudioNext

# Prev
"playerctl previous"
    XF86AudioPrev
